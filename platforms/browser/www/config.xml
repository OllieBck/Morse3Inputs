<?xml version='1.0' encoding='utf-8'?>
<widget id="info.jasonbeck" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Morse2Go</name>
    <description>
        Morse Code application built from Mor.js and ResponsiveVoice JS v1.5.1.  Inspired by work Jim Wroten at http://morse2go.org/.
    </description>
    <author email="jason@adapativedesign.org" href="http://www.adaptivedesign.org">
        Adaptive Design Associaiton
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Fullscreen" value="true" />
    <preference name="BackgroundColor" value="0xff0000ff" />
    <preference name="Orientation" value="landscape" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
</widget>
